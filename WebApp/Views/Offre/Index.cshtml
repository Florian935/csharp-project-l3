@model List<WebApp.Models.OffreViewModel>


@{
    ViewBag.Title = "Offre";
}

<h2>Offre</h2>
@using (Html.BeginForm())
{
    <p>
        Find by name: @Html.TextBox("SearchString")
        <input type="submit" value="Search" />
    </p>
}
<ul>
    @foreach (var item in Model)
    {
        <li>
            <div>@item.Intitule</div>
            <button><a href="@Url.Action("Detail", "Offre", new { id = item.Id })">Détails</a></button>
        </li>
    }
</ul>

